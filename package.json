{
  "name": "face-to-guy",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "vue3-NutUI4",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "NODE_ENV=production npm run build:weapp -- --watch ",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "eslint:fix": "eslint --ext .vue --ext .js --ext .jsx --ext .ts --ext .tsx --fix src/",
    "eslint": "eslint --ext .vue --ext .js --ext .jsx --ext .ts --ext .tsx src/",
    "stylelint": "stylelint **/*.{css,less,scss,styl}",
    "lint": "npm run eslint && npm run stylelint",
    "lint-staged": "lint-staged --allow-empty",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx,.vue",
    "prepare": "husky install",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "cz": "cz",
    "release": "standard-version --prerelease alpha",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": [
      "npm run lint-staged:js",
      "npm run tsc"
    ],
    "**/*.{less,css,scss}": "npm run stylelint",
    "**/*.{js,jsx,ts,tsx,vue,less,css,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "zhuzi",
  "keywords": [
    "vue3",
    "taro",
    "pinia",
    "miniproress"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@nutui/icons-vue-taro": "^0.0.9",
    "@nutui/nutui-taro": "^4.0.7",
    "@tarojs/components": "3.6.6",
    "@tarojs/components-advanced": "^3.6.7",
    "@tarojs/helper": "3.6.6",
    "@tarojs/plugin-framework-vue3": "3.6.6",
    "@tarojs/plugin-html": "3.6.6",
    "@tarojs/plugin-platform-alipay": "3.6.6",
    "@tarojs/plugin-platform-h5": "3.6.6",
    "@tarojs/plugin-platform-jd": "3.6.6",
    "@tarojs/plugin-platform-qq": "3.6.6",
    "@tarojs/plugin-platform-swan": "3.6.6",
    "@tarojs/plugin-platform-tt": "3.6.6",
    "@tarojs/plugin-platform-weapp": "3.6.6",
    "@tarojs/runtime": "3.6.6",
    "@tarojs/shared": "3.6.6",
    "@tarojs/taro": "3.6.6",
    "lottie-miniprogram": "^1.0.12",
    "pinia": "^2.0.35",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@dcasia/mini-program-tailwind-webpack-plugin": "^1.5.6",
    "@tarojs/cli": "3.6.6",
    "@tarojs/plugin-vue-devtools": "3.6.6",
    "@tarojs/webpack5-runner": "3.6.6",
    "@types/webpack-env": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-preset-taro": "3.6.6",
    "c8": "^7.11.3",
    "commitizen": "^4.2.4",
    "css-loader": "3.4.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-taro": "3.6.6",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.0",
    "happy-dom": "^6.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "style-loader": "1.3.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "^3.7.0",
    "unplugin-vue-components": "^0.23.0",
    "vue-loader": "^17.0.0",
    "webpack": "5.69.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
